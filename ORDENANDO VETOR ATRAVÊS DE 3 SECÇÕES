#include <stdio.h>
#include <math.h>

int main(){

    int n;
    scanf("%d",&n);
    int tam;
    tam=(3*n)+3;
    int l[tam];
   
    for(int i=0;i<tam;i++){
        if( i==n || i==(2*n)+1  || i==(3*n)+2 ){l[i]=99999999;}
        else {scanf("%d",&l[i]);}
    }
    for(int i=0;i<tam;i++){
         
        printf("%d ",l[i]);    
   
       
    }
   
    int i,j,k,M[3*n],x;
   
    i=0;j=n+1;k=(2*n)+2;
   
    for(x=0;x<(3*n);x++){
       
        if (l[i]<l[j] && l[i]<l[k]){
            M[x]=l[i];
            i++;
        }
        else if(l[j]<l[i] && l[j]<l[k]){
            M[x]=l[j];
            j++;
           
        }
        else if(l[k]<l[i] && l[k]<l[j]){
            M[x]=l[k];
            k++;
        }
       
     
    }
   
    printf("\n");
    for(x=0;x<(3*n);x++){
        printf("%d ",M[x]);
    }
    return 0;
}
